{
  "name": "Muse by TweetMaestro (DEV)",
  "action": {
    "default_icon": "public/dev-icon-32.png",
    "default_popup": "src/pages/popup/index.html"
  },
  "icons": {
    "128": "public/dev-icon-128.png"
  },
  "host_permissions": [
    "http://localhost:3000/*",
    "http://localhost:8000/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "http://localhost:8000/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "dev-icon-128.png",
        "dev-icon-32.png"
      ],
      "matches": []
    }
  ]
}
